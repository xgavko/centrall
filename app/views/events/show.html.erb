<%= @event.status = 'voting' %>
<%= @event.name %>
<%= @event.status %>

<% if @event.boarding? %>
  <%= render 'boarding', event: @event %>
<% elsif @event.voting? %>
  <%= render 'voting', event: @event %>
<% elsif @event.display_result? %>
  <%= render 'display_result', event: @event %>
<% end %>
