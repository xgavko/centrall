<div
  id="map"
  style="width: 100%;
  height: 50vh;"
  data-marker='<%= raw @event.chosen_place.coordinates.to_json %>'
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
></div>


<div class="result">
  <div class="result-body">

    <h1 class="result-title"><%= @event.name %></h1>
